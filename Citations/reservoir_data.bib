	journal = {International Journal for Parasitology: Parasites and Wildlife},
	author = {Catalano, S. and Nadler, S. A. and Fall, C. B. and Marsh, K. J. and Leger, E. and Sene, M. and Priestnall, S. L. and Wood, C. L. and Diouf, N. D. and Ba, K. and Webster, J. P.},
