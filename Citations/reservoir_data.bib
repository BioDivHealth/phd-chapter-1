	keywords = {Bartonella, Borrelia, Senegal, article, controlled study, nonhuman, female, male, animal experiment, genotype, gene amplification, Theileria, sex difference, bacterium identification, *zoonosis, filariasis, body mass, *infection risk, taxonomy, microbial community, *host pathogen interaction, Anaplasma, species habitat, borrelia crocidurae, *rodent, Anaplasmataceae, Bartonella raoultii, DNA screening, Ehrlichia, Ehrlichia senegalensis, Hepatozoon, Kinetoplastida, native species, Piroplasmida, spleen tissue, Theileria senegalensis},
	pages = {202},
