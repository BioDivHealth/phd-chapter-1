@article{modry_two_2008,
	title = {Two new species of \textit{{Eimeria}} {Schneider}, 1875 ({Apicomplexa}: {Eimeriidae}) from \textit{{Gerbilliscus} guineae} {Thomas} ({Rodentia}: {Gerbillinae}) in the {Niokolo} {Koba} {National} {Park}, {Senegal}},
