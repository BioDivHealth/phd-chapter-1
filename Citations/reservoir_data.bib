	pages = {313--318},
	annote = {The following values have no corresponding Zotero field:auth-address: F.A. Stewart, Leverhulme Centre for Human Evolutionary Studies, Department of Biological Anthropology, University of Cambridge, Fitzwilliam Street, CB2 1QH, United Kingdom. E-mail: fas31@cam.ac.ukpub-location: United Statespublisher: Wiley-Liss Inc. (111 River Street, Hoboken NJ 07030-5774, United States)accession-num: 51573751},
