@article{sall-dram_variation_2010,
	title = {Variation in cestode assemblages of mastomys and arvicanthis species ({Rodents}: {Muridae}) from {Lake} {Retba} in {Western} {Senegal}},
	volume = {96},
	issn = {0022-3395 1937-2345},
	doi = {https://dx.doi.org/10.1645/GE-2437.1},
	abstract = {We studied patterns of variation in cestode communities of 3 abundant rodent species that live in sympatry in the Niayes of the Retba Lake, Western Senegal. We evaluated whether the host species have the same parasites and, within host species, whether the variability in parasite community is related to intrinsic (sex, age of the host individual) or extrinsic (habitat, season) factors. Arvicanthis niloticus was parasitized by 2 cestode species, namely Inermicapsifer madagascariensis and the highly dominant Raillietina trapezodes. Raillietina baeri was the only cestode species found in Mastomys erythroleucus, and there was no cestode in M. huberti. Prevalence and abundance levels of cestodes were very high in A. niloticus, especially in adults. Seasonal and habitat effects were found in both cestode communities of M. erythroleucus and A. niloticus. Local host specificity and abundance/prevalence levels suggested variations in the interaction between rodents and cestode intermediate host species among habitats and seasons. © 2010 American Society of Parasitologists.},
	language = {English},
	number = {4},
	journal = {Journal of Parasitology},
	author = {Sall-Dram, R. and Brouat, C. and Ba, C. T. and Duplantier, J. M.},
	year = {2010},
	keywords = {Senegal, article, nonhuman, female, male, prevalence, season, *Muridae, age, sex, species habitat, *Mastomys, *cestode, *Arvicanthis},
	pages = {675--680},
	annote = {The following values have no corresponding Zotero field:auth-address: R. Sall-Dram, IRD (UR 022), Centre de Biologie et de Gestion des Populations, BP 1386, Dakar, CP 18524, Senegal. E-mail: sallro@ird.snpub-location: United Statespublisher: American Society of Parasitologists (P.O. Box 1897, Lawrence KS 66044-8897, United States)accession-num: 359448983},
