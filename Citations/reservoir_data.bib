	year = {2017},
	keywords = {human, Senegal, article, nonhuman, phylogeny, sequence analysis, geographic distribution, reverse transcription polymerase chain reaction, virus isolation, virus strain, public health, gene amplification, rat, sequence alignment, life cycle, *virus identification, *Flavivirus, *wesselsbron virus},
	pages = {23--28},
	annote = {The following values have no corresponding Zotero field:auth-address: A.A. Sall, Institut Pasteur de Dakar, Dakar, Senegal. E-mail: asall@pasteur.snpub-location: Netherlandspublisher: Elsevier B.V.accession-num: 614473112},
