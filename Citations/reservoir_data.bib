}

@article{bichet_sanitary_2003,
	title = {Sanitary and zootechnical impact of clinical coccidiosis upon laying hen in {Senegal}},
	volume = {154},
	issn = {0035-1555},
	abstract = {This study has been carried out from October 1999 to October 2001 on a thirty livestock sample, randomly selected, representative of the semi-industriel chain of production in Senegal. The results revealed that 90 \% of the flocks were infected by coccidia and 23 \% expressed clinical symptoms. Three species of Eimeria were revealed : E. acervulina, E. maxima and E. tenella. In this population the mortality rate due to coccidiosis reaches 10.8 \%. Coccidiosis was responsible for the death of 2.0 \% of the animal population on the point of laying which represented 18.9 \% of the total mortality and induced a 5.4 egg fall per laying hen, i.e. 2.5 \%. With regards to flocks free from clinical coccidiosis, the consumption index of attacked livestock was 19 \% higher and the culling 12 weeks delayed. Eventually, there was no significant difference between weight of culling hens with and without history of clinical symptoms.},
