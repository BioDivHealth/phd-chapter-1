	journal = {Journal of Medical Entomology},
	author = {Boulanger, Denis and Doucoure, Souleymane and Grout, Lise and Ngom, Abdoulaye and Rogerie, Francois and Cornelie, Sylvie and Sokhna, Cheikh and Mouchet, Francois and Riveau, Gilles and Simondon, Francois and Remoue, Franck J.},
