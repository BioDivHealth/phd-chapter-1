@article{wane_sahelian_2010,
	title = {Sahelian herders and the markets: {Evidence} from the {Senegalese} {Sahel} ({Ferlo})},
	volume = {19},
	issn = {1166-7699},
	doi = {10.1684/agr.2009.0329},
	abstract = {Often perceived as non-economic contemplative beings, the pastoral populations of Ferlo appeal to the markets in order to sell their products. Their main goal for selling livestock and animal products consists in making money to cover their usual expenses and to ensure their food safety. Based on primary data obtained through investigations in Ferlo encampments throughout a whole year (winter 2005 - winter 2006), a progressive decomposition of their trade incomes shows that the drillings, often considered by policies as an element which structures the pastoral activity, does not systematically improve the money holdings. The released incomes are related to the marketing of ruminants (97.9\% of the livestock farming income with the bovines contributing for 40\% and small ruminants for 60\%). The sale of dairy by-products (fresh and curdled milk, butter) remain marginal (0.50\%) and are conversely linked to the degree of enclosure of the herders' places of residence and activity. The diversification of production and the activities is not apparent in the activities of selling. The concepts of poverty, vulnerability and supply of basic social services are reconsidered according to the characteristics of pastoralism in arid regions (mobility, families breakdown, spatial dispersion).},
	number = {1},
	journal = {Cahiers Agricultures},
	author = {Wane, A. and Ancey, V. and Touré, I.},
	month = feb,
	year = {2010},
	pages = {14--20},
