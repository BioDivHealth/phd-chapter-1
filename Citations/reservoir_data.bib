	pages = {1798--1819},
	annote = {The following values have no corresponding Zotero field:auth-address: R. Pouillot, 7403 Wyndale Lane, Chevy Chase, MD 20815, United States. E-mail: rpouillot@yahoo.frpub-location: United Statespublisher: Blackwell Publishing Inc. (350 Main Street, Malden MA 02148, United States)accession-num: 51959045},
