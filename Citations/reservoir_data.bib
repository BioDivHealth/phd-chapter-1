	author = {Mustapha, O. A. and Teriba, E. E. and Ezekiel, O. S. and Olude, A. M. and Akinloye, A. K. and Olopade, J. O.},
	year = {2020},
	keywords = {Senegal, Ghana, nonhuman, review, *livestock, male, animal experiment, Nigeria, breeding, South Africa, rat, parasitology, Medline, telecommunication, search engine, anesthesia, *rodent, *wildlife, biochemistry, developmental biology, hematology, knowledge gap, toxicology},
	pages = {40--46},
