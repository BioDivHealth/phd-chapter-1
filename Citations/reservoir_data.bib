	author = {Claxton, J. R. and Faye, J. A. and Rawlings, P.},
	year = {1992},
	keywords = {Animals, Female, Insect Vectors, Male, Prevalence, Gambia/ep [Epidemiology], Trypanosomiasis, African/ep [Epidemiology], *Trypanosomiasis, African/ve [Veterinary], Trypanosomiasis, African/bl [Blood], Animals, Wild, Antibodies, Protozoan/bl [Blood], Tsetse Flies, Swine, *Swine Diseases/ep [Epidemiology], Swine Diseases/bl [Blood], Trypanosoma/im [Immunology]},
