@article{maca_description_2012,
	title = {Description of a new species of \textit{{Eimeria}} {Schneider}, 1875 ({Apicomplexa}: {Eimeriidae}) from the western {Derby} eland \textit{{Taurotragus} derbianusderbianus} {Gray} ({Artiodactyla}: {Bovidae}) in {Senegal}},
