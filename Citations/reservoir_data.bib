	keywords = {Animals, Female, Humans, Seasons, Cattle, *Trypanosomiasis, African, Gambia/ep [Epidemiology], *Trypanosomiasis, Lactation, Farmers, Milk},
	pages = {106520},
	annote = {The following values have no corresponding Zotero field:section: Olaniyan, Olawale F. Department of Animal Science, Faculty of Agriculture, Ege University, Izmir 35040, Turkey; West Africa Livestock Innovation Centre, PMB 14, Banjul, Gambia. Electronic address: of.olaniyan@utg.edu.gm. Secka, Arss. West Africa Livestock Innovation Centre, PMB 14, Banjul, Gambia. Kaya, Ibrahim. Department of Animal Science, Faculty of Agriculture, Ege University, Izmir 35040, Turkey.pub-location: Netherlands},
