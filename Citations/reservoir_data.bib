}

@article{camara_determinants_2019,
	title = {Determinants of breeders' participation to an indigenous cattle breeding program},
	volume = {39},
	issn = {1774-0746 1773-0155},
	doi = {10.1007/s13593-019-0591-1},
	abstract = {Many cattle breeding programs were initiated in Africa to increase the productivity of indigenous cattle breeds. Most of these programs have failed, partly due to the lack of involvement of breeders. The present case study contributes to the understanding of such failures. The N'Dama cattle breeding program in Senegal was taken as a case study for an in-depth analysis of participation using mixed methods. Semi-structured interviews were conducted with 52 breeders: 26 who participated, 15 of whom recently resigned, and 27 who had never participated. Content and statistical analyses were conducted to evaluate the motivations of breeders and the factors influencing their participation in the breeding program. Results more particularly highlight the complexity of social issues within a breeding project, in face of classical determinants of adoption that are distance or production systems features. It pinpoints crucial levers of improvement, i.e., the management of animal property rights between the nucleus management and the participating breeders, the legitimacy of participants' representatives in cooperatives, and the strategic mobilization of member social networks. Also, adding on previous works of the authors, this study highlights the need to take better account of the dynamics of production systems, then paying sufficient attention to the objectives, preferences, and ongoing strategies of the breeders for the future. The present study is the first to highlight the added value of mixed methods to analyze innovation adoption and participation in a livestock breeding program, taking both into account the overall innovation drivers and dynamics tied to actors' strategies.},
	number = {5},
	journal = {Agronomy for Sustainable Development},
	author = {Camara, Y. and Ciss, M. and Moula, N. and Sissokho, M. M. and Farnir, F. and Antoine-Moussiaux, N.},
	month = oct,
	year = {2019},
	annote = {The following values have no corresponding Zotero field:accession-num: WOS:000482228100001},
}

@misc{arbelot_development_1999,
	title = {Development and organization of poultry farming around {Dakar}. {I}. {Organizing} a diagnosis laboratory and an animal health monitoring service},
	abstract = {As in many west African countries, peri-urban poultry farming in Senegal has considerably developed in response to growing demand for animal proteins. To support the development of the sector, the Projet de Developpement des Elevages a Cycle Court (PRODEC - short-cycle livestock rearing development project) was launched in the Dakar region in February 1994, for a four-year period. Amongst other objectives, PRODEC set out to install a poultry analysis laboratory at ISRA-LNERV (pathology and feeds) and a training structure managed by a Board representing the professionals involved in the sector ("Maisons des Aviculteurs"). After three and a half years of operations, the project has succeeded in setting up a fee-paying poultry pathology laboratory, introducing surveys to ensure greater control of on-farm pathologies, and training technicians (laboratory and rearing technicians), poultry farmers and veterinary surgeons. The project has encountered difficulties with industrialists concerning health checks on one-day-old chicks and the creation of an interprofessional structure capable of managing and running an aviculture training centre.},
	author = {Arbelot, B. and Dayon, J. F. and Merouani, N.},
	year = {1999},
