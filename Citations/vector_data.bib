@article{dickson_aedes_2011,
	title = {Aedes aegypti formosus is not a single subspecies in {Senegal}, {West} {Africa}},
	volume = {85},
	issn = {0002-9637},
	abstract = {Aedes aegypti is the major vector of the Dengue, Yellow Fever, and Chikungunya viruses. The subspecies Aedes aegypti aegypti (Aaa) has a global distribution while Aedes aegypti formosus (Aaf) is limited to Sub-Saharan Africa. We used McClelland's scoring of the frst abdominal tergite to subdivide Aaf collected in Senegal into form {\textless}{\textgreater} (no white scales) and form {\textless}{\textgreater} (white scales present). Average fecundity was observed within F or G+ forms but few eggs and very few larvae were produced in F x G+ crosses. Analysis of the Argo2 gene identifed distinct SNPS associated with F or G+ forms. {\textless}{\textgreater} markers predominated in the southeastern, forested regions of Senegal and the {\textless}{\textgreater}.markers predominated in western and northern Senegal. Analysis of genotype frequencies demonstrated that 29.2\% (31/106) of tests departed signifcantly from HWE and 25 of the 31 signifcant tests had an excess of homozygotes. Wahlund's effect is one explanation for this and occurs when a collection is sampled as a single reproductive pool, when there are actually two or more reproductively isolated populations. When the Aaf {\textless}{\textgreater} were analyzed separately, genotypes ft HWE. However, when Aaf {\textless}{\textgreater} was analyzed, an excess of homozygotes continued to be observed; suggesting that the {\textless}{\textgreater} form may consist of more than one reproductively isolated subspecies. Linkage disequilibrium analysis of SNPs in cDNAs was performed on 10 different populations from Senegal, and 10 populations from throughout the world. There was a large amount of disequilibrium among alleles on all three chromosomes. We tested for suppression of recombination associated with inversions that had been previously detected in Aaf. We crossed {\textless}{\textgreater} males with Higgs White Eye (HWE) Aaa females and then backcrossed male F1 to HWE females. We anticipated fnding suppressed recombination or that the we locus would be 14cM from the Sex locus as previously reported. Instead the loci were 14cM apart in some families but unlinked in others suggesting a translocation. All of these results suggest that Aaf is comprised of multiple subspecies in West Africa that can be distinguished based upon chromosomal polymorphisms and molecular genetic markers.},
	language = {English},
	number = {6 SUPPL. 1},
	journal = {American Journal of Tropical Medicine and Hygiene},
	author = {Dickson, L. and Caspary, A. and Moore, M. and Sylla, M. and Black, W. C.},
	year = {2011},
