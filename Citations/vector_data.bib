@article{mattioli_experimental_1994,
	title = {Experimental {Trypanosoma} congolense infection on naturally occurring ticks in {N}'dama and {Gobra} zebu cattle},
	volume = {36},
	issn = {0048-2951},
	abstract = {The effects of experimental Trypanosoma congolense infection in Gambian N'dama and Gobra zebu cattle on number of naturally-occurring adult ticks attaching were studied. An indirect fluorescent antibody test was performed to detect serological prevalence of Cowdria ruminantium antibody. The intravenously imposed trypanosome infection did not result in significant (P {\textgreater} 0.05) differences in Amblyomma variegatum and Hyalomma spp. infestations between control and infected N'dama cattle. Control N'damas carried significantly (P {\textless} 0.001) lower numbers of A. variegatum and Hyalomma spp. than the control zebus. Serological frequency of C. ruminantium antibody was similar in both control or infected N'dama and in control or infected zebu cattle. No deaths occurred among N'dama cattle, while all six trypanosome infected zebus progressively died within nine weeks post-infection but trypanosomosis was excluded as the primary cause of death. Examined Giemsa-stained blood smears were negative for the presence of tick-borne micro-organisms. Four positive cases of cowdriosis were identified during post-mortem examination. It was concluded that N'damas, even when submitted to trypanosome infection, react consistently better than Gobra zebus to tick attachment. These results emphasize the benefits of rearing disease resistant cattle breeds, such as N'dama, in areas where risks of trypanosomosis and cowdriosis coexist.},
	number = {3},
	journal = {Parassitologia},
	author = {Mattioli, R. C. and Faye, J. A. and Bah, M. and Jabang, B.},
	year = {1994},
	keywords = {Animals, Gambia, Ticks/mi [Microbiology], Species Specificity, Ticks/cl [Classification], *Trypanosomiasis, African/ve [Veterinary], Cattle Diseases/tm [Transmission], Heartwater Disease/tm [Transmission], *Cattle Diseases/ps [Parasitology], Antibodies, Bacterial/bl [Blood], *Tick Infestations/ve [Veterinary], *Cattle/ps [Parasitology], Trypanosomiasis, African/co [Complications], Genetic Predisposition to Disease, *Arachnid Vectors/ph [Physiology], *Ticks/ph [Physiology], *Trypanosoma congolense/ph [Physiology], Arachnid Vectors/mi [Microbiology], Arachnid Vectors/ps [Parasitology], Cattle Diseases/mi [Microbiology], Cattle/cl [Classification], Ehrlichia ruminantium/im [Immunology], Heartwater Disease/co [Complications], Heartwater Disease/im [Immunology], Heartwater Disease/pa [Pathology], Ivermectin/tu [Therapeutic Use], Tick Infestations/co [Complications], Ticks/ps [Parasitology]},
