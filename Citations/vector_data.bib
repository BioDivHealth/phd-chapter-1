	keywords = {dengue, population, Senegal, yellow fever, Africa south of the Sahara, female, male, *hygiene, *society, *tropical medicine, nucleotide, *Aedes aegypti, Mexico, gene, intron, phenotype, Thailand, genetic distance, single nucleotide polymorphism, chromosome, Flavivirus, subspecies, library, competence, *gene flow, *genome, genetic linkage},
	pages = {20},
