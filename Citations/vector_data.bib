	year = {2007},
	keywords = {Gambia, article, *tick, female, male, cattle, animal, genetics, microbiology, comparative study, disease transmission, methodology, *heartwater disease, *Ehrlichia ruminantium, *polymerase chain reaction},
	pages = {61--74},
	annote = {The following values have no corresponding Zotero field:auth-address: B. Faburay, Department of Infectious Diseases and Immunology, Faculty of Veterinary Medicine, Utrecht University, Yalelaan 1, Utrecht 3584CL, Netherlands. E-mail: b.fabureh1@vet.uu.nlpub-location: Netherlandspublisher: Springer Netherlands (Van Godewijckstraat 30, Dordrecht 3311 GZ, Netherlands)accession-num: 46911398},
}

@article{vial_molecular_2006,
	title = {Molecular divergences of the {Ornithodoros} sonrai soft tick species, a vector of human relapsing fever in {West} {Africa}},
	volume = {8},
	issn = {1286-4579 1769-714X},
	doi = {https://dx.doi.org/10.1016/j.micinf.2006.07.012},
	abstract = {The soft tick Ornithodoros sonrai is recognized as the only vector of Borrelia crocidurae causing human relapsing fever in West Africa. Its determination has been exclusively based on morphological features, geographical distribution and vector competence. Some ambiguities persist in its systematics and may cause misunderstanding about West African human relapsing fevers epidemiology. By amplifying and aligning 16S and 18S rDNA genes in O. sonrai specimens collected from 14 distinct sites in Senegal and Mauritania, we showed the existence of four genetically different subgroups that were morphologically and ecologically identified as belonging to the same species. Within O. sonrai, intraspecific polymorphism was high (pairwise divergence from 0.2\% to 16.4\%). In all cases, these four subgroups formed a monophyletic clade sharing a common ancestor with East African soft ticks that transmit Borrelia duttoni human relapsing fever. From amplification of the flagellin gene of B. crocidurae we verified that all subgroups of O. sonrai were infected by B. crocidurae and may constitute vectors for this pathogen. All flagellin sequences were identical, refuting the hypothesis suggesting parallel evolution between O. sonrai and B. crocidurae. However, differences in infection rates were significant, suggesting different vector competences between subgroups of O. sonrai. © 2006 Elsevier Masson SAS. All rights reserved.},
	language = {English},
	number = {11},
