	keywords = {Senegal, article, controlled study, nonhuman, female, male, *mating, humidity, age distribution, egg production, *Glossina, *Glossina palpalis gambiensis, *Emergence, *low temperature, *process technology, *pupa, *sterile insect technique, *survival, cage, copulation, insemination, organism colony, rating scale, Relative Mating Index, storage temperature},
	pages = {465},
	annote = {The following values have no corresponding Zotero field:auth-address: G.N. Mutika, Insect Pest Control Laboratory, Joint FAO/IAEA Division of Nuclear Techniques in Food and Agriculture, International Atomic Energy Agency, Vienna A-1400, Austria. E-mail: gratian.mutika@gmail.compub-location: United Kingdompublisher: BioMed Central Ltd. (E-mail: info@biomedcentral.com)accession-num: 612350911},
