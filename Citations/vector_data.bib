@article{sagna_spatial_2019,
	title = {Spatial {Assessment} of {Contact} {Between} {Humans} and {Anopheles} and {Aedes} {Mosquitoes} in a {Medium}-{Sized} {African} {Urban} {Setting}, {Using} {Salivary} {Antibody}-{Based} {Biomarkers}},
	volume = {220},
	issn = {1537-6613 0022-1899},
	doi = {https://dx.doi.org/10.1093/infdis/jiz289},
	abstract = {BACKGROUND: Anarchic and poorly controlled urbanization led to an increased risk of mosquito-borne diseases (MBD) in many African cities. Here, we evaluate the spatial heterogeneity of human exposure to malaria and arboviral disease vectors in an urban area of northern Senegal, using antibody-based biomarkers of exposure to Anopheles and Aedes mosquito bites., METHODS: A cross-sectional study was undertaken during the rainy season of 2014 in 4 neighborhoods of Saint-Louis, a city in northern Senegal. Among children aged 6-59 months in each neighborhood, the dried blood spot technique was used to evaluate immunoglobulin G (IgG) responses to both gSG6-P1 (Anopheles) and Nterm-34-kDa (Aedes) salivary peptides as validated biomarkers of respective mosquito bite exposure., RESULTS: IgG response levels to gSG6-P1 and Nterm-34-kDa salivary peptides varied significantly between the 4 neighborhoods (P {\textless} .0001). The level of exposure to Aedes bites also varied according to household access to sanitation services (P = .027), whereas that of exposure to Anopheles bites varied according to insecticide-treated bed net use (P = .006). In addition, spatial clusters of high contact between humans and mosquitoes were identified inside 3 neighborhoods., CONCLUSIONS: Antibody-based biomarkers of exposure to Anopheles and Aedes mosquito bites could be helpful tools for evaluating the heterogeneity of exposure to malaria and arboviral disease vectors by national control programs. Copyright © The Author(s) 2019. Published by Oxford University Press for the Infectious Diseases Society of America. All rights reserved. For permissions, e-mail: journals.permissions@oup.com.},
	number = {7},
	journal = {The Journal of infectious diseases},
	author = {Sagna, Andre B. and Kassie, Daouda and Couvray, Agnes and Adja, Akre Maurice and Hermann, Emmanuel and Riveau, Gilles and Salem, Gerard and Fournet, Florence and Remoue, Franck},
	year = {2019},
	keywords = {Animals, Female, Humans, Child, Preschool, Infant, Male, Cross-Sectional Studies, Senegal/ep [Epidemiology], *Malaria/ep [Epidemiology], Incidence, Developing Countries, Immunoglobulin G/bl [Blood], Plasmodium, *Aedes/im [Immunology], Malaria/tm [Transmission], Biomarkers/bl [Blood], *Anopheles/im [Immunology], *Salivary Proteins and Peptides/im [Immunology], *Insect Bites and Stings/im [Immunology], *Insect Proteins/im [Immunology], *Mosquito Vectors/im [Immunology], Cities, Dried Blood Spot Testing},
	pages = {1199--1208},
	annote = {The following values have no corresponding Zotero field:section: Sagna, Andre B. MIVEGEC, University of Montpellier, CNRS, IRD. Sagna, Andre B. Institut Pierre Richet/Institut National de Sante Publique, Bouake, Cote d'Ivoire. Sagna, Andre B. Biomedical Research Center Espoir Pour La Sante, Saint-Louis, Senegal. Kassie, Daouda. ASTRE, University of Montpellier, CIRAD, INRA, Montpellier. Kassie, Daouda. Institut de Recherche en Sciences de la Sante, Bobo-Dioulasso, Burkina Faso. Couvray, Agnes. CEPED, University of Paris Descartes, IRD, Paris. Adja, Akre Maurice. Institut Pierre Richet/Institut National de Sante Publique, Bouake, Cote d'Ivoire. Hermann, Emmanuel. CIIL, Institut Pasteur de Lille, University of Lille, CNRS UMR, Inserm, Lille, France. Riveau, Gilles. CIIL, Institut Pasteur de Lille, University of Lille, CNRS UMR, Inserm, Lille, France. Riveau, Gilles. Biomedical Research Center Espoir Pour La Sante, Saint-Louis, Senegal. Salem, Gerard. CEPED, University of Paris Descartes, IRD, Paris. Fournet, Florence. MIVEGEC, University of Montpellier, CNRS, IRD. Fournet, Florence. Institut de Recherche en Sciences de la Sante, Bobo-Dioulasso, Burkina Faso. Remoue, Franck. MIVEGEC, University of Montpellier, CNRS, IRD.pub-location: United States},
