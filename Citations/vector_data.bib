@article{gaye_vector_2020,
	title = {Vector competence of anthropophilic mosquitoes for a new mesonivirus in {Senegal}},
	volume = {9},
	issn = {2222-1751},
	doi = {https://dx.doi.org/10.1080/22221751.2020.1730710},
	abstract = {The mesoniviruses (MESOVs) belong to the newly described Mesoniviridae family (Order: Nidovirales). They have never been reported in Senegal until recently during a study in arbovirus emergence with the detection of a new species of MESOV named Dianke virus (DKV) from common mosquitoes from eastern Senegal. Actually, their vector competence for this newly described DKV is unknown. We, therefore, estimated the vector competence of Culex tritaeniorhynchus, Culex quinquefasciatus, Aedes aegypti, and Anopheles gambiae mosquitoes collected in Senegal for DKV using oral infection. Whole bodies, legs/wings, and saliva samples were tested for DKV by RT-PCR to estimate infection, dissemination, and transmission rates. The infectivity of virus particles in the saliva was confirmed by infecting C6/36 cells. Virus transmission rates were up to 95.45\% in Culex tritaeniorhynchus, 28\% in Cx. quinquefasciatus and 9.09\% in Aedes aegypti. Viral particles in the saliva were confirmed infectious by C6/36 cell culture. An. gambiae was able to disseminate DKV only at 20 days post-infection. This study shows that Culex mosquitoes are more competent than Ae. aegypti for DKV, while Anopheles gambiae is not likely a competent vector.},
	number = {1},
	journal = {Emerging Microbes \& Infections},
	author = {Gaye, Alioune and Diagne, Moussa Moise and Ndiaye, El Hadji and Dior Ndione, Marie Henriette and Faye, Martin and Talla, Cheikh and Fall, Gamou and Ba, Yamar and Diallo, Diawo and Dia, Ibrahima and Handschumacher, Pascal and Faye, Ousmane and Sall, Amadou Alpha and Diallo, Mawlouth},
	year = {2020},
	keywords = {Senegal, Animals, Female, Saliva/vi [Virology], *Aedes/vi [Virology], Cell Line, *Mosquito Vectors, *Nidovirales, Nidovirales Infections/tm [Transmission]},
	pages = {496--504},
	annote = {The following values have no corresponding Zotero field:section: Gaye, Alioune. Unite d'Entomologie Medicale, Institut Pasteur de Dakar, Dakar, Senegal. Diagne, Moussa Moise. Pole de virologie, Unite des Arbovirus et virus de Fievres Hemorragiques, Institut Pasteur de Dakar, Dakar, Senegal. Ndiaye, El Hadji. Unite d'Entomologie Medicale, Institut Pasteur de Dakar, Dakar, Senegal. Dior Ndione, Marie Henriette. Pole de virologie, Unite des Arbovirus et virus de Fievres Hemorragiques, Institut Pasteur de Dakar, Dakar, Senegal. Dior Ndione, Marie Henriette. Universite Cheikh Anta Diop de Dakar, Dakar, Senegal. Faye, Martin. Pole de virologie, Unite des Arbovirus et virus de Fievres Hemorragiques, Institut Pasteur de Dakar, Dakar, Senegal. Talla, Cheikh. Epidemiology of infectious diseases unit, Institut Pasteur de Dakar, Dakar, Senegal. Fall, Gamou. Pole de virologie, Unite des Arbovirus et virus de Fievres Hemorragiques, Institut Pasteur de Dakar, Dakar, Senegal. Ba, Yamar. Unite d'Entomologie Medicale, Institut Pasteur de Dakar, Dakar, Senegal. Diallo, Diawo. Unite d'Entomologie Medicale, Institut Pasteur de Dakar, Dakar, Senegal. Dia, Ibrahima. Unite d'Entomologie Medicale, Institut Pasteur de Dakar, Dakar, Senegal. Handschumacher, Pascal. Aix Marseille Univ, INSERM, IRD, UMR SESSTIM, Sciences Economiques \& Sociales de la Sante \& Traitement de l'Information Medicale, Marseille, France. Faye, Ousmane. Pole de virologie, Unite des Arbovirus et virus de Fievres Hemorragiques, Institut Pasteur de Dakar, Dakar, Senegal. Sall, Amadou Alpha. Pole de virologie, Unite des Arbovirus et virus de Fievres Hemorragiques, Institut Pasteur de Dakar, Dakar, Senegal. Diallo, Mawlouth. Unite d'Entomologie Medicale, Institut Pasteur de Dakar, Dakar, Senegal.pub-location: United States},
