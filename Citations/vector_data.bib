@article{vignolles_terrasar-x_2010,
	title = {{TerraSAR}-{X} high-resolution radar remote sensing: an operational warning system for {Rift} {Valley} fever risk},
	volume = {5},
	issn = {1970-7096 1827-1987},
	abstract = {In the vicinity of the Barkedji village (in the Ferlo region of Senegal), the abundance and aggressiveness of the vector mosquitoes for Rift Valley fever (RVF) are strongly linked to rainfall events and associated ponds dynamics. Initially, these results were obtained from spectral analysis of high-resolution ({\textasciitilde}10 m) Spot-5 images, but, as a part of the French AdaptFVR project, identification of the free water dynamics within ponds was made with the new high-resolution (down to 3-meter pixels), Synthetic Aperture Radar satellite (TerraSAR-X) produced by Infoterra GmbH, Friedrichshafen/Potsdam, Germany. During summer 2008, within a 30 x 50 km radar image, it was found that identified free water fell well within the footprints of ponds localized by optical data (i.e. Spot-5 images), which increased the confidence in this new and complementary remote sensing technique. Moreover, by using near real-time rainfall data from the Tropical Rainfall Measuring Mission (TRMM), NASA/JAXA joint mission, the filling-up and flushing-out rates of the ponds can be accurately determined. The latter allows for a precise, spatio-temporal mapping of the zones potentially occupied by mosquitoes capable of revealing the variability of pond surfaces. The risk for RVF infection of gathered bovines and small ruminants ({\textasciitilde}1 park/km(2)) can thus be assessed. This new operational approach (which is independent of weather conditions) is an important development in the mapping of risk components (i.e. hazards plus vulnerability) related to RVF transmission during the summer monsoon, thus contributing to a RVF early warning system.},
	number = {1},
	journal = {Geospatial Health},
	author = {Vignolles, Cecile and Tourre, Yves M. and Mora, Oscar and Imanache, Laurent and Lafaye, Murielle},
	year = {2010},
	keywords = {Animals, Humans, Senegal/ep [Epidemiology], *Rift Valley Fever/ep [Epidemiology], Risk Assessment, Rift Valley Fever/tm [Transmission], Algorithms, Zoonoses/ep [Epidemiology], Rain/ps [Parasitology], *Culicidae, *Climate, Epidemiologic Methods, Disease Outbreaks/sn [Statistics \& Numerical Data], Zoonoses/ps [Parasitology], *Geographic Information Systems/is [Instrumentation], *Remote Sensing Technology/is [Instrumentation], Models, Theoretical, Remote Sensing Technology/mt [Methods], Tropical Medicine},
	pages = {23--31},
	annote = {The following values have no corresponding Zotero field:section: Vignolles, Cecile. Centre National d'Etudes Spatiales (CNES), 18 Avenue Edouard Belin, 31401 Toulouse Cedex 9, France.pub-location: Italy},
