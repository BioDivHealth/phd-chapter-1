	number = {5 SUPPL. 1},
	journal = {American Journal of Tropical Medicine and Hygiene},
	author = {Caputo, B. and Nwakanma, D. and Caputo, F. P. and Jawara, M. and Oriero, C. E. and Hamid-Adiamoh, M. and Dia, I. and Konate, L. and Petrarca, V. and Pinto, J. and Conway, D. and Della Torre, A.},
	year = {2014},
	keywords = {human, malaria, population, Gambia, Africa, Africa south of the Sahara, genetic variability, microsatellite marker, river, *hygiene, *society, *tropical medicine, mosquito, epidemiology, species, habitat, *Anopheles gambiae, genetic marker, chromosome inversion, model, chromosome, taxon, reproductive isolation, X chromosome, implantable cardiac monitor, genetic difference, *hybridization, *gene flow, chromosome 2, habitat fragmentation},
